{
  "plugins": [
    "@typescript-eslint",
    "tailwindcss"
  ],
  "extends": [
    "next/core-web-vitals",
    "plugin:@typescript-eslint/recommended",
    "prettier",
    "plugin:tailwindcss/recommended"
  ],
  "rules": {
    "@typescript-eslint/no-unused-vars": 0,
    "@typescript-eslint/no-explicit-any": "error",
    "react/no-unescaped-entities": 0
  }
}
